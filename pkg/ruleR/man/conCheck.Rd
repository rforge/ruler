## The function is currently defined as
function (seq) 
{
    if (class(seq) != "list") 
        stop("sequence must be of type 'list'")
    m <- length(seq)
    if (identical(seq[m], seq[m - 1]) && identical(seq[m], seq[m - 
        2])) {
        return(0)
    }
    else {
        return(1)
    }
  }
}
